{
  "name": "faciem",
  "version": "1.0.0",
  "private": true,
  "description": "The Frontend of the venovum application",
  "author": "BjÃ¶rn Pfoster",
  "scripts": {
    "build": "nuxt build",
    "test": "npm run test:unit && npm run test:e2e",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "dev": "nuxt",
    "generate": "nuxt generate",
    "generate:tailwind": "npx tailwind build assets/css/tailwind.css -o assets/css/output.css -c tailwind.config.js",
    "precommit": "npm run lint",
    "start": "nuxt start",
    "start:ci": "nuxt build -c nuxt.config.ci.js && nuxt start -c nuxt.config.ci",
    "test:e2e": "npm run build && npx start-test start:ci :3000 test:e2e:cypress",
    "test:e2e:cypress": "cypress run",
    "test:e2e:cypress:record": "cypress run --record --key $CYPRESS_KEY",
    "test:unit": "jest",
    "deploy": "npm run build && sls deploy",
    "start:server": "npm run build && node server.js",
    "start:sls": "npm run build && sls offline start"
  },
  "dependencies": {
    "@ky-is/tailwind-color-palette": "^1.0.0",
    "@nuxtjs/axios": "^5.6.0",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@nuxtjs/eslint-module": "^0.0.1",
    "moment": "^2.24.0",
    "nuxt": "^2.9.1",
    "nuxt-start": "^2.9.1",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-http": "^2.2.0",
    "tailwindcss": "^1.1.2",
    "vue-router": "^3.1.2",
    "vuex": "latest"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.9.0",
    "cypress": "^3.4.1",
    "eslint": "^5.15.1",
    "eslint-config-standard": "^14.0.0",
    "eslint-plugin-cypress": "^2.6.1",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.9",
    "sass-loader": "^7.3.1",
    "serverless-domain-manager": "^3.3.0",
    "serverless-offline": "^4.10.0",
    "start-server-and-test": "^1.10.0",
    "style-loader": "^0.23.1",
    "vue-jest": "^3.0.3"
  }
}
