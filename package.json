{
  "name": "faciem",
  "version": "1.0.0",
  "private": true,
  "description": "The Frontend of the venovum application",
  "author": "BjÃ¶rn Pfoster",
  "scripts": {
    "build": "npm run build:beta",
    "build:beta": "NODE_ENV=beta nuxt build --pwa",
    "build:production": "NODE_ENV=production nuxt build --pwa",
    "docker": "docker run -it -p 5000:5000 faciem",
    "docker:build": "docker build -t faciem .",
    "test": "npm run test:unit && npm run test:e2e",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "locale": "npx vue-i18n-extract report -v './!(node_modules|dist)/**/*.?(js|vue)' -l './assets/lang/*.json' -a",
    "dev": "nuxt",
    "generate": "nuxt generate",
    "generate:tailwind": "npx tailwind build assets/css/tailwind.css -o assets/css/output.css -c tailwind.config.js",
    "precommit": "npm run lint",
    "start": "nuxt start",
    "start:ci": "nuxt build -c nuxt.config.ci.js && nuxt start -c nuxt.config.ci",
    "test:e2e": "npm run build && npx start-test start:ci :3000 test:e2e:cypress",
    "test:e2e:cypress": "cypress run",
    "test:e2e:cypress:record": "cypress run --record --key $CYPRESS_KEY",
    "test:unit": "jest"
  },
  "dependencies": {
    "@ky-is/tailwind-color-palette": "^1.0.0",
    "@nuxtjs/axios": "^5.10.3",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/toast": "^3.3.0",
    "dinero.js": "^1.8.1",
    "jwt-decode": "^2.2.0",
    "lodash.clonedeep": "^4.5.0",
    "moment": "^2.26.0",
    "network-speed": "^2.0.6",
    "nuxt": "^3.0.0",
    "nuxt-i18n": "^6.12.2",
    "nuxt-start": "^2.12.2",
    "tailwindcss": "^1.4.6",
    "video.js": "^7.8.2",
    "videojs-resolution-switcher": "^0.4.2",
    "vue-awesome": "^4.0.2",
    "vue-router": "^3.3.2",
    "vuex": "^3.4.0",
    "vuex-persistedstate": "^2.7.1"
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "cypress": "^3.8.3",
    "eslint": "^5.15.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-nuxt": "^0.5.2",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "glob": "^7.1.6",
    "jest": "^24.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^1.19.4",
    "sass-loader": "^13.2.0",
    "start-server-and-test": "^1.10.8",
    "style-loader": "^3.3.1",
    "vue-i18n-extract": "^1.1.1",
    "vue-jest": "^4.0.1"
  }
}
